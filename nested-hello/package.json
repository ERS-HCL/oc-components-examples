{
  "name": "nested-hello",
  "description": "This shows how to nest components published in the same registry",
  "version": "1.0.0",
  "repository": "https://github.com/matteofigus/oc-components-examples",
  "author": "Matteo Figus <matteofigus@gmail.com>",
  "oc": {
    "files": {
      "data": "server.js",
      "template": {
        "src": "template.jade",
        "type": "jade"
      }
    },
    "parameters": {
      "name": {
        "type": "string",
        "mandatory": false,
        "example": "John Doe",
        "description": "Your name"
      }
    }
  }
}
